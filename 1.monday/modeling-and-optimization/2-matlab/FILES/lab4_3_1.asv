X = [1 2 3 4 5 6];
Y = [5.2 6.3 7.1 8.5 9.2 10.0];
xlabel('X');
ylabel('Y');
hold on
plot(X,Y,'ko')
t = -3 : 0.1 : 3;
c2 = polyfit(X,Y,1);
y2 = polyval(c2,t);
plot(t, y2, 'k:')
c4 = polyfit(X,Y,4);
y4 = polyval(c4,t);
plot(t, y4, 'k-')
title('Plynom approximation');
legend('Data','poly 2 degree','poly 4 degree',0)